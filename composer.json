{
  "name": "contestio/magento1",
  "type": "magento-module",
  "license": "proprietary",
  "description":"Plugin Contestio pour Magento 1.x",
  "version": "1.0.19",
  "authors":[
      {
          "name":"Contestio",
          "email":"contact@contestio.fr"
      }
  ],
  "require": {
      "magento-hackathon/magento-composer-installer": "^4.0"
  },
  "autoload": {
    "psr-0": {
      "Contestio_Connect": "app/code/local/"
    }
  },
  "extra": {
      "magento-deploy-ignore": [
          "*.git*",
          "*.md",
          "composer.json",
          "composer.lock"
      ],
      "magento-force": true,
      "magento-root-dir": ".",
      "with-bootstrap-patch": false
  },
  "config": {
      "allow-plugins": {
          "magento-hackathon/magento-composer-installer": true
      }
  }
} 