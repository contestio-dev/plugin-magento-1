{
  "name": "contestio/magento1",
  "type": "magento-module",
  "license": "proprietary",
  "description": "Plugin Contestio pour Magento 1.x",
  "version": "1.0.21",
  "authors": [
      {
          "name": "Contestio",
          "email": "contact@contestio.fr"
      }
  ],
  "require": {
      "magento-hackathon/magento-composer-installer": "^3.0"
  },
  "extra": {
      "magento-deploy-ignore": [
          "*.git*",
          "*.md",
          "composer.json",
          "composer.lock"
      ],
      "magento-force": true,
      "magento-root-dir": "./",
      "magento-deploystrategy": "copy",
      "map": [
          ["app", "app"]
      ]
  },
  "config": {
      "allow-plugins": {
          "magento-hackathon/magento-composer-installer": true
      }
  }
}
